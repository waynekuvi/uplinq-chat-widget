{
  "name": "uplinq-chat-widget",
  "version": "1.0.0",
  "description": "Embeddable Uplinq support chat widget packaged for reuse across sites.",
  "type": "module",
  "main": "dist/uplinq-chat-widget.umd.js",
  "module": "dist/uplinq-chat-widget.es.js",
  "exports": {
    ".": {
      "import": "./dist/uplinq-chat-widget.es.js",
      "default": "./dist/uplinq-chat-widget.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run postbuild",
    "postbuild": "mkdir -p dist && cp -f public/index.html dist/index.html",
    "preview": "vite preview --mode library"
  },
  "keywords": [
    "uplinq",
    "chatbot",
    "widget",
    "n8n",
    "support"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
  }
}

